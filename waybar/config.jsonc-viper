    "custom/rdplist": {
        "format": " ÓÆ©  ",
        "on-click": "~/.dotfiles/scripts/rdplist.sh",
        "tooltip": false
    },

    "cpu": {
        "interval": 10,
        "format": "Ôãõ  {usage}%",
        "max-length": 10,
        "on-click": "gnome-system-monitor"

    },

    "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
        "activated": "ÔÅÆ  I'M WIDE AWAKE!!",
        "deactivated": "ÔÅ∞ "
    },
    "timeout": 30.5
    },

    "custom/publicip": {
        "format": " ÔÇ¨   {} ",
        "tooltip": false,
        "exec": "exec ~/.dotfiles/scripts/net.sh publicip",
    },

     "custom/gateway": {
        "format": " Û∞ë™   {} ",
        "tooltip": false,
        "exec": "exec ~/.dotfiles/scripts/net.sh gateway",
    },

    "custom/dns": {
        "format": " Û∞áñ   {} ",
        "tooltip": false,
        "exec": "exec ~/.dotfiles/scripts/net.sh dns",
    },

    "custom/netreset": {
        "format": "Û∞±ì  ",
        "tooltip": false,
        "on-click": "exec ~/.dotfiles/scripts/setupnic.sh viper",
    },

    "memory": {
        "interval": 30,
        "format": "ÔÉâ  {}%",
        "max-length": 10,
        "on-click": "gnome-system-monitor"
    },

    "network#WiFi": {
        "interface": "wlan0",
        "format": "{ifname}",
        "format-wifi": "Ôá´   {essid} ({ipaddr}) ",
        "format-disconnected": "", //An empty format will hide the module.
        "tooltip-format": "{ifname}",
        "tooltip-format-wifi": "{essid} ({signalStrength}%)",
        "tooltip-format-disconnected": "Disconnected",
        "max-length": 50,
        "on-click": "nm-connection-editor",
        "on-click-right": "nm-applet"
    },

    "network#Office": {
        "interface": "bridge1",
        "format": "{ifname}",
        "format-ethernet": "ECM: {ipaddr} ",
        "format-disconnected": "", //An empty format will hide the module.
        "tooltip-format": "{ifname}",
        "tooltip-format-ethernet": "{ifname} ÔÉÅ",
        "tooltip-format-disconnected": "Disconnected",
        "max-length": 50,
        "on-click": "nm-connection-editor",
        "on-click-right": "nm-applet"
    },

    "network#Domain": {
        "interface": "bridge0",
        "format": "{ifname}",
        "format-ethernet": "Domain: {ipaddr} ",
        "format-disconnected": "", //An empty format will hide the module.
        "tooltip-format": "{ifname}",
        "tooltip-format-ethernet": "{ifname} ÔÉÅ",
        "tooltip-format-disconnected": "Disconnected",
        "max-length": 50,
        "on-click": "nm-connection-editor",
        "on-click-right": "nm-applet"
    },

    "network#SAN": {
        "interface": "enp12s0f1",
        "format": "{ifname}",
        "format-ethernet": "SAN: {ipaddr} ",
        "format-disconnected": "", //An empty format will hide the module.
        "tooltip-format": "{ifname}",
        "tooltip-format-ethernet": "{ifname} ÔÉÅ",
        "tooltip-format-disconnected": "Disconnected",
        "max-length": 50,
        "on-click": "nm-connection-editor",
        "on-click-right": "nm-applet"
    },

    "network#Setup": {
        "interface": "enp4s0",
        "format": "{ifname}",
        "format-ethernet": "Setup: {ipaddr} ",
        "format-disconnected": "", //An empty format will hide the module.
        "tooltip-format": "{ifname}",
        "tooltip-format-ethernet": "{ifname} ÔÉÅ",
        "tooltip-format-disconnected": "Disconnected",
        "max-length": 50,
        "on-click": "nm-connection-editor",
        "on-click-right": "nm-applet"
    },


    "custom/wireguard-manager": {
    "exec": "exec ~/.dotfiles/scripts/wireguard-manager/wireguard-manager.sh -s",
    "format": "{icon}", 
    "format-icons": {
        "connected": "<span color=\"#50fa7b\"> Û±éö  ECM-MGMT</span>", /*üîíüîì*/
        "disconnected": "<span color=\"#5a5a5a\">Û±ô±  ECM-MGMT</span>",
    },
    "interval": "once",
    "on-click": "~/.dotfiles/scripts/wireguard-manager/wireguard-manager.sh -t && pkill -SIGRTMIN+1 waybar",
    "return-type": "json",
    "signal": 1,
    },

   "custom/powermenu" : {
        "format": "ÔÄë ",
        "tooltip" : true,
        "on-click": "~/.dotfiles/scripts/powermenu.sh"
    },

    "custom/insync" : {
        "exec": "exec ~/.dotfiles/scripts/insync.sh",
        "format": "{}",
        "on-click": "insync show",
        "interval": 30,
        "tooltip": false
    },

    "tray": {
        "icon-size": 18,
        "spacing": 10
    },
    

    "clock#date": {
        "format": "{:%d.%m.%Y}",
        "tooltip": false
    },

    "clock": {
        "format": "{:%R}",
        "tooltip": false
    },

    "backlight": {
        "device": "intel_backlight",
        "format": "{icon} {percent}%",
        "format-icons": ["Ôóù"],
        "on-scroll-up": "brightnessctl set 5%+",
        "on-scroll-down": "brightnessctl set 5%-",
        "min-length": 6
    },
    "battery": {
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 20
        },
        "format": "{icon} {capacity}%",
        "format-charging": "Ôá¶ {capacity}%",
        "format-plugged": "Ôá¶ {capacity}%",
        "format-alt": "{time} {icon}",
        "format-icons": ["ÔâÑ  ","ÔâÉ  ", "ÔâÅ  ", "ÔâÄ  "]
    },
    "pulseaudio": {
        "format": "{icon} {volume}% ",
        "tooltip": false,
        "format-muted": "ÔÄ¶ Muted ",
        "on-click": "pamixer -t",
        "on-click-right": "pavucontrol",
        "on-scroll-up": "pamixer -i 5",
        "on-scroll-down": "pamixer -d 5",
        "scroll-step": 5,
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "ÔÄ•",
            "headset": "ÔÄ•",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶ ", "ÔÄß ", "ÔÄ® "]
        }
    },
    "pulseaudio#microphone": {
        "format": "{format_source}",
        "format-source": "Ô°´ {volume}%",
        "format-source-muted": "Ô°¨ Muted",
        "on-click": "pamixer --default-source -t",
        "on-scroll-up": "pamixer --default-source -i 5",
        "on-scroll-down": "pamixer --default-source -d 5",
        "scroll-step": 5
    },

     // Taskbar
    "wlr/taskbar": {
        "format": "{icon}",
        "icon-size": 18,
        "tooltip-format": "{title}",
        "on-click": "activate",
        "icon-theme": "Newaita-reborn-darkQ"
    },

    // Bluetooth
    "bluetooth": {
        "format-disabled": "Û∞Ç≤ ",
        "format-off": "Û∞Ç≤ ",
        "format-on": "Û∞ÇØ ",
        "format-connected": "Û∞Ç∞ ",
        "interval": 30,
        "on-click": "blueman-manager"
    },


    // Group Hardware
    "group/hardware": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 300,
            "children-class": "not-memory",
            "transition-left-to-right": false
        },        
        "modules": [
            "memory",
            "cpu"
            
        ]
    },

    // Group Sound/Backlight
     "group/soundbl": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 300,
            "children-class": "not-memory",
            "transition-left-to-right": false
        },        
        "modules": [
            "pulseaudio"
            
        ]
    },

    // Group Time/Date
     "group/timedate": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 300,
            "children-class": "not-memory",
            "transition-left-to-right": false
        },        
        "modules": [
            "clock",
            "clock#date"
            
        ]
    },

    // Group networking
     "group/net": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 300,
            "children-class": "not-memory",
            "transition-left-to-right": true
        },        
        "modules": [
            "custom/publicip",
            "custom/gateway"
            
        ]
    },

