    "custom/launcher": {
        "format": "Û∞£á ",
        "interval": "once",

        "on-click": "~/.dotfiles/scripts/applauncher.sh",

        "tooltip": false
    },

    
    "custom/filelauncher": {
        "format": " Ôìì  ",
        "on-click": "nemo",
        "tooltip": false
    },

    "temperature": {
        "thermal-zone": 1,
        "format": "{icon} {temperatureC}¬∞C",
        "format-icons": ["Ôãã", "Ôãä", "Ôãâ", "Ôãà", "Ôãá"],
        "tooltip": false
    },

    "custom/terminallauncher": {
        "format": " Ó™Ö  ",
